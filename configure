    fi
    if test "$with_system_libcmis" = "yes"; then
        as_fn_error $? "--with-system-libcmis conflicts with --enable-dbgutil" "$LINENO" 5
    else
        with_system_libcmis=no
    fi
    if test "$enable_win_mozab_driver" = "yes"; then
        as_fn_error $? "--enable-win-mozab-driver conflicts with --enable-dbgutil" "$LINENO" 5
    fi
else
    ENABLE_DBGUTIL=""
    MSVC_USE_DEBUG_RUNTIME=""
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
