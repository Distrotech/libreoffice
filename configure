COM_GCC_IS_CLANG=
if test "$GCC" = "yes"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether GCC is actually Clang" >&5
$as_echo_n "checking whether GCC is actually Clang... " >&6; }

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

        #ifndef __clang__
        you lose
        #endif
        int foo=42;

_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
         COM_GCC_IS_CLANG=TRUE
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

    if test "$COM_GCC_IS_CLANG" = TRUE; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: checking the Clang version" >&5
$as_echo_n "checking the Clang version... " >&6; }
        clang_version=`echo __clang_major__.__clang_minor__.__clang_patchlevel__ | $CC -E -P -`
        CLANG_FULL_VERSION=`echo __clang_version__ | $CC -E -P -`
        CLANGVER=`echo $clang_version | $AWK -F. '{ print \$1*10000+\$2*100+\$3 }'`
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: Clang $CLANG_FULL_VERSION, $CLANGVER" >&5
$as_echo "Clang $CLANG_FULL_VERSION, $CLANGVER" >&6; }
        cat >>confdefs.h <<_ACEOF
#define CLANG_FULL_VERSION $CLANG_FULL_VERSION
_ACEOF

    fi
fi


if test "$CCACHE" != "" -a "$COM_GCC_IS_CLANG" = TRUE; then
    if test -z "$CCACHE_CPP2"; then
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Using ccache with Clang without CCACHE_CPP2 set causes spurious warnings." >&5
$as_echo "$as_me: WARNING: Using ccache with Clang without CCACHE_CPP2 set causes spurious warnings." >&2;}
            add_warning "Using ccache with Clang without CCACHE_CPP2 set causes spurious warnings."
    fi
fi

if test "$GCC" = "yes" -a -z "$COM_GCC_IS_CLANG"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking the GCC version" >&5
$as_echo_n "checking the GCC version... " >&6; }
    _gcc_version=`$CC -dumpversion`
    GCC_VERSION=`echo $_gcc_version | $AWK -F. '{ print \$1*100+\$2 }'`

    if test "$_os" = "Darwin"; then
        if test "$with_macosx_sdk" = "10.5"; then
            # use gcc 4.2 for OS X SDK 10.5
            if test -z "$save_CC" -a -x "$GCC_HOME/bin/gcc-4.2"; then
                export CC="$GCC_HOME/bin/gcc-4.2" # make CC finally available to config.guess
            fi
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: using CC=$CC" >&5
$as_echo "using CC=$CC" >&6; }
        fi
    else
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: gcc $_gcc_version" >&5
$as_echo "gcc $_gcc_version" >&6; }
    fi

    if test "$GCC_VERSION" -lt 0401; then
        as_fn_error $? "GCC $_gcc_version is too old, must be at least GCC 4.1.0" "$LINENO" 5
    fi
else
    # Explicitly force GCC_VERSION to be empty, even for Clang, to check incorrect uses.
    # GCC version should generally be checked only when handling GCC-specific bugs, for testing
    # things like features configure checks should be used, otherwise they may e.g. fail with Clang
    # (which reports itself as GCC 4.2.1).
    GCC_VERSION=
fi


