{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether to do a debug build" >&5
$as_echo_n "checking whether to do a debug build... " >&6; }
if test -n "$enable_debug" && test "$enable_debug" != "yes" && test "$enable_debug" != "no"; then
    as_fn_error $? "--enable-debug now accepts only yes or no, use --enable-selective-debuginfo" "$LINENO" 5
