if test -n "${GIT_REFERENCE_SRC}"; then
    for repo in ${GIT_NEEDED_SUBMODULES}; do
        if ! test -d "${GIT_REFERENCE_SRC}"/${repo}; then
            as_fn_error $? "referenced git: required repository does not exist: ${GIT_REFERENCE_SRC}/${repo}" "$LINENO" 5
        fi
    done
fi


