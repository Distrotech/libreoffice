msgctxt ""
"06060000.xhp\n"
"par_idN107A5\n"
"help.text"
msgid "An optional + character signals that the text paragraphs are animated."
msgstr "An optional + character signals that the text paragraphs are animated."

#: 06060000.xhp
msgctxt ""
"06060000.xhp\n"
"par_idN107A9\n"
"help.text"
