msgid "<link href=\"text/simpress/02/10120000.xhp\" name=\"Lines and Arrows\">Lines and Arrows</link>"
msgstr "<link href=\"text/simpress/02/10120000.xhp\" name=\"Lines and Arrows\">线条和箭头</link>"

