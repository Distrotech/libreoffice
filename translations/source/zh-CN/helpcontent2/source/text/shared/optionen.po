"serverauthentication.xhp\n"
"par_idN105E5\n"
"help.text"
msgid "User name"
msgstr "用户名"

#: serverauthentication.xhp
msgctxt ""
"serverauthentication.xhp\n"
"par_idN105EA\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the user name for the SMTP server.</ahelp>"
