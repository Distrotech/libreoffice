"help.text"
msgid "Colors"
msgstr "颜色"

#: 01010500.xhp
msgctxt ""
"01010500.xhp\n"
"bm_id3155132\n"
"help.text"
msgid "<bookmark_value>colors; models</bookmark_value>"
msgstr "<bookmark_value>颜色; 模型</bookmark_value>"

#: 01010500.xhp
msgctxt ""
"01010500.xhp\n"
"hd_id3150543\n"
"1\n"
"help.text"
