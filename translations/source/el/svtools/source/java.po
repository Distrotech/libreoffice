"string.text"
msgid "Select JRE"
msgstr "Επιλογή JRE"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_ERROR_JVMCREATIONFAILED_TITLE\n"
"string.text"
msgid "JRE is Defective"
msgstr "Το JRE είναι προβληματικό"
