"01040500.xhp\n"
"par_id3150447\n"
"30\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/opttablepage/header\">Specifies that the first row of the table is formatted with the \"Table heading\" Paragraph Style.</ahelp>"
msgstr ""

