msgctxt ""
"view.src\n"
"STR_ERROR_SAVE_TEMPLATE\n"
"string.text"
msgid "Error saving template "
msgstr "ཊེམ་པེལེཊི་ སྲུང་བཞག་འབད་ནི་འཛོལ་བ།"

#: view.src
msgctxt ""
"view.src\n"
"STR_READONLY\n"
