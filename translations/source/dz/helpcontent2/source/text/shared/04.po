#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"hd_id3150208\n"
"51\n"
"help.text"
msgid "Shift+<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Tab"
msgstr "སོར་ལྡེ་+<switchinline select=\"sys\"><caseinline select=\"MAC\">བརྡ་བཀོད་ </caseinline><defaultinline>ཚད་འཛིན་</defaultinline></switchinline>+མཆོང་ལྡེ།"

