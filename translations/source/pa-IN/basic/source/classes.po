msgstr "'$(ARG1)' ਫਾਇਲ ਵਿੱਚੋਂ BASIC ਸ਼ੁਰੂ ਨਹੀਂ ਕੀਤਾ ਜਾ ਸਕਦਾ ਹੈ।"

#: sb.src
msgctxt ""
"sb.src\n"
"ERRCODE_BASMGR_MGRSAVE & ERRCODE_RES_MASK\n"
"string.text"
msgid "Error saving BASIC: '$(ARG1)'."
msgstr "BASIC ਸੰਭਾਲਣ ਵਿੱਚ ਗਲਤੀ: '$(ARG1)'"

#: sb.src
msgctxt ""
