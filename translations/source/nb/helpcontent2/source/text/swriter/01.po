"par_id3147217\n"
"20\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/frmurlpage/name\">Enter a name for the hyperlink.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/frmurlpage/name\">Skriv inn navnet du vil bruke på lenka.</ahelp>"

