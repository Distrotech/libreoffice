msgctxt ""
"05060100.xhp\n"
"par_id3146949\n"
"66\n"
"help.text"
msgid "<link href=\"text/shared/01/05260000.xhp\" name=\"Format - Anchor\"><emph>Format - Anchor</emph></link>"
msgstr "<link href=\"text/shared/01/05260000.xhp\" name=\"Format - Anchor\"><emph>Format → Forankring</emph></link>"

