msgctxt ""
"conn_error_message.src\n"
"256 + 2*301 + 0\n"
"string.text"
msgid "$1$ is no SQL conform identifier."
msgstr "$1$ कोई SQL अनुरूपता पन्छानू नेईं ऐ."

