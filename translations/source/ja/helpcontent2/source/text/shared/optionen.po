"hd_id3152960\n"
"48\n"
"help.text"
msgid "Character set"
msgstr "文字コードセット"

#: 01030500.xhp
msgctxt ""
"01030500.xhp\n"
"par_id3149018\n"
"49\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/opthtmlpage/charset\">Select the appropriate character set for the export</ahelp>."
msgstr ""

