msgctxt ""
"01020000.xhp\n"
"par_id3156368\n"
"105\n"
"help.text"
msgid "Select the object in front of the currently selected object."
msgstr "Selecciona o obxecto colocado diante do seleccionado neste momento."

#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"hd_id3148879\n"
"106\n"
"help.text"
