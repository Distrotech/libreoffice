msgctxt ""
"euro.src\n"
"STEP_CONVERTER\n"
"string.text"
msgid "~Entire document"
msgstr "මුළු ලේඛනයම (~E)"

#: euro.src
msgctxt ""
"euro.src\n"
"STEP_CONVERTER + 1\n"
"string.text"
