"string.text"
msgid "~Remember password until end of session"
msgstr "Энэ суултыг дуусах хүртэл нууц үгийг ~сануул"

#: dbinteraction.src
msgctxt ""
"dbinteraction.src\n"
"STR_REMEMBERPASSWORD_PERSISTENT\n"
"string.text"
msgid "~Remember password"
msgstr "Нууц үгийг ~сануул"
