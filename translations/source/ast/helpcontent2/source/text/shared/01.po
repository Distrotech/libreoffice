"hd_id3154673\n"
"3\n"
"help.text"
msgid "<link href=\"text/shared/01/04060100.xhp\" name=\"Select Source\">Select Source</link>"
msgstr "<link href=\"text/shared/01/04060100.xhp\" name=\"Select Source\">Escoyer fonte</link>"

