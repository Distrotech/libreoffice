msgctxt ""
"03020404.xhp\n"
"par_id3153416\n"
"27\n"
"help.text"
msgid "sDir=\"Directories:\""
msgstr "sDir=\"Directoris:\""

#: 03020404.xhp
msgctxt ""
"03020404.xhp\n"
"par_id3154253\n"
"34\n"
"help.text"
msgid "' Get the directories"
msgstr "' Obtindre els directoris"

