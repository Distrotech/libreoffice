"par_id3147560\n"
"4\n"
"help.text"
msgid "Choose (Index, Selection1[, Selection2, ... [,Selection_n]])"
msgstr "Choose (Index, Selection1[, Selection2, ... [,Selection_n]])"

#: 03090402.xhp
