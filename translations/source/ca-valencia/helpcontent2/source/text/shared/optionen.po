"par_id3150440\n"
"6\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/tpviewpage/formula\">Specifies whether to show formulas instead of results in the cells.</ahelp>"
msgstr ""

#: 01060100.xhp
msgctxt ""
"01060100.xhp\n"
"hd_id3155132\n"
"7\n"
"help.text"
msgid "Zero values"
msgstr "Valors zero"

