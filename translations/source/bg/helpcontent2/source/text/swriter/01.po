msgctxt ""
"04090200.xhp\n"
"par_id3151078\n"
"39\n"
"help.text"
msgid "x == \"ABC\" or x EQ \"ABC\""
msgstr "x == \"ABC\" или x EQ \"ABC\""

#: 04090200.xhp
msgctxt ""
"04090200.xhp\n"
"par_id3150162\n"
"40\n"
"help.text"
msgid "Checks if variable \"x\" contains (true) the \"ABC\" string, or not (false)."
msgstr "Проверява дали променливата „x“ съдържа низа \"ABC\" (истина), или не (лъжа)."

#: 04090200.xhp
msgctxt ""
"04090200.xhp\n"
"par_id3153301\n"
"41\n"
"help.text"
msgid "x == \"\" or x EQ \"\""
msgstr "x == \"\" или x EQ \"\""

#: 04090200.xhp
msgctxt ""
"04090200.xhp\n"
"par_id3156120\n"
"90\n"
"help.text"
msgid "or"
msgstr "или"

