msgctxt ""
"04120212.xhp\n"
"hd_id3152950\n"
"32\n"
"help.text"
msgid "Sort"
msgstr "Сортиране"

#: 04120212.xhp
msgctxt ""
"04120212.xhp\n"
"par_id3149812\n"
"33\n"
"help.text"
msgid "Sets the options for sorting the index entries."
msgstr "Определя настройките за сортиране на записите за указател."

