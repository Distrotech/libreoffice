"par_id1717886\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Resizes the object to the original size.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Преоразмерява обекта до оригиналния му размер.</ahelp>"

