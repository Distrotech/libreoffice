msgctxt ""
"about_meta_tags.xhp\n"
"par_id3154908\n"
"6\n"
"help.text"
msgid "Keywords"
msgstr "Ключови думи"

#: about_meta_tags.xhp
msgctxt ""
"about_meta_tags.xhp\n"
"par_id3156422\n"
"7\n"
"help.text"
msgid "<META NAME=\"KEYWORDS\" CONTENT=\"Field Content\">"
msgstr "<META NAME=\"KEYWORDS\" CONTENT=\"Съдържание на полето\">"

#: about_meta_tags.xhp
msgctxt ""
"about_meta_tags.xhp\n"
"par_id3151041\n"
"8\n"
"help.text"
msgid "Description"
msgstr "Описание"

#: about_meta_tags.xhp
msgctxt ""
"about_meta_tags.xhp\n"
"par_id3125863\n"
"9\n"
"help.text"
msgid "<META NAME=\"DESCRIPTION\" CONTENT=\"Field Content\">"
msgstr "<META NAME=\"DESCRIPTION\" CONTENT=\"Съдържание на полето\">"

