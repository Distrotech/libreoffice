msgctxt ""
"05050000.xhp\n"
"par_id3150623\n"
"6\n"
"help.text"
msgid "<ahelp hid=\".\">Toggles case of all selected western characters.</ahelp>"
msgstr "<ahelp hid=\".\">Превключва регистъра на всички избрани западни знаци.</ahelp>"

