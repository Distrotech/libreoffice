msgstr "<ahelp hid=\".\">Променя първата буква от всяка дума в главна на избраните западни знаци.</ahelp>"

#: 05050000.xhp
msgctxt ""
"05050000.xhp\n"
"hd_id3147521\n"
"5\n"
"help.text"
msgid "tOGGLE cASE"
msgstr "оБРАТЕН рЕГИСТЪР"

