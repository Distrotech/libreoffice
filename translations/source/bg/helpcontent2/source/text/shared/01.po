msgstr "Тема"

#: about_meta_tags.xhp
msgctxt ""
"about_meta_tags.xhp\n"
"par_id3147228\n"
"5\n"
"help.text"
msgid "<META NAME=\"CLASSIFICATION\" CONTENT=\"Field Content\">"
msgstr "<META NAME=\"CLASSIFICATION\" CONTENT=\"Съдържание на полето\">"

