msgctxt ""
"05290300.xhp\n"
"par_id3148548\n"
"help.text"
msgid "<link href=\"text/shared/01/05290400.xhp\" name=\"Exit Group\">Exit Group</link>"
msgstr "<link href=\"text/shared/01/05290400.xhp\" name=\"Излизане от група\">Излизане от група</link>"

