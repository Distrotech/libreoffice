"06050000.xhp\n"
"par_id3153961\n"
"9\n"
"help.text"
msgid "Stop"
msgstr "Stoppen"

#: 06050000.xhp
msgctxt ""
"06050000.xhp\n"
"par_id3147297\n"
"12\n"
"help.text"
msgid "<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_PLAY\">Plays the animation.</ahelp>"
msgstr "<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_PLAY\">Klik op deze knop om de animatie te starten.</ahelp>"

