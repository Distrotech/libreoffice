msgctxt ""
"06050000.xhp\n"
"par_id3153119\n"
"help.text"
msgid "<image id=\"img_id3149352\" src=\"res/helpimg/right.png\" width=\"0.2189in\" height=\"0.1874in\"><alt id=\"alt_id3149352\">Icon</alt></image>"
