msgctxt ""
"03132000.xhp\n"
"par_id3150359\n"
"40\n"
"help.text"
msgid "oLib.addContainerListener( oListener ) ' Register the listener"
msgstr "oLib.addContainerListener( oListener ) ' Registrar a Listener"

#: 03132000.xhp
