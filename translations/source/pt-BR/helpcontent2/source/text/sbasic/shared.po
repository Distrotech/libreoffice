msgctxt ""
"01170101.xhp\n"
"hd_id3154663\n"
"121\n"
"help.text"
msgid "Date max."
msgstr "Data máx."

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"par_id3148485\n"
"122\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the upper limit for a date control.</ahelp>"
msgstr "<ahelp hid=\".\">Especifique o limite superior para um controle de data.</ahelp>"

#: 01170101.xhp
