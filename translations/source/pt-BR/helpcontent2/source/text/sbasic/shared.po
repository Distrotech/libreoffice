msgctxt ""
"03102100.xhp\n"
"par_id3154662\n"
"29\n"
"help.text"
msgid "DIM text(20) as String REM 21 elements numbered from 0 to 20"
msgstr "DIM text(20) as String REM 21 elementos numerados de 0 a 20"

