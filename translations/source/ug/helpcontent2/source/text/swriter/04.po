msgctxt ""
"01020000.xhp\n"
"bm_id3155593\n"
"help.text"
msgid "<bookmark_value>removing; cell protection in text documents</bookmark_value>"
msgstr "<bookmark_value>چىقىرىۋەت; تېكست پۈتۈكتىكى كاتەكچە قوغداش</bookmark_value>"

