msgctxt ""
"03020407.xhp\n"
"par_id3154124\n"
"4\n"
"help.text"
msgid "FileDateTime (Text As String)"
msgstr "FileDateTime (teksti1 As String)"

#: 03020407.xhp
msgctxt ""
"03020407.xhp\n"
