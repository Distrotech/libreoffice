msgctxt ""
"03020301.xhp\n"
"par_id3153360\n"
"19\n"
"help.text"
msgid "Print #iNumber, \"First line of text\""
msgstr "Print #iNumber, \"Tämä on tekstirivi.\""

#: 03020301.xhp
msgctxt ""
"03020301.xhp\n"
"par_id3148797\n"
"20\n"
"help.text"
msgid "Print #iNumber, \"Another line of text\""
msgstr "Print #iNumber, \"Toinen rivi tekstiä.\""

#: 03020302.xhp
msgctxt ""
"03020302.xhp\n"
"tit\n"
