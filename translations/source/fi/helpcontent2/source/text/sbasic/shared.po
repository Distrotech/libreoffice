msgctxt ""
"03020203.xhp\n"
"par_id3147124\n"
"18\n"
"help.text"
msgid "Print #iNumber, \"This is a line of text\""
msgstr "Print #iNumber, \"Tämä on tekstirivi.\""

#: 03020203.xhp
msgctxt ""
"03020203.xhp\n"
"par_id3153415\n"
"19\n"
"help.text"
msgid "Print #iNumber, \"This is another line of text\""
msgstr "Print #iNumber, \"Tässä on toinen rivi tekstiä\""

