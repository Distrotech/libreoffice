msgctxt ""
"03030205.xhp\n"
"par_id3147349\n"
"14\n"
"help.text"
msgid "12, 61, 45 corresponds to 13, 2, 45"
msgstr "(12, 61, 45 vastaa lukemia 13, 1, 45)"

