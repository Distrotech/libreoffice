"par_id3148664\n"
"5\n"
"help.text"
msgid "Declare {Sub | Function} Name Lib \"Libname\" [Alias \"Aliasname\"] [Parameter] [As Type]"
msgstr "Declare {Sub | Function} nimi1 Lib \"kirjastonimi\" [Alias \"aliasnimi\"] [argumenttiluettelo] [As tyyppi1]"

