#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"hd_id3157900\n"
"6\n"
"help.text"
msgid "Go to next slide"
msgstr "སྒྲོན་བཪྙན་སྤྱིན་ཤོག་རྗེས་མར་བསྒྱུར་བ་"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"par_id3150655\n"
"47\n"
"help.text"
msgid "Moves forward one slide in the slide show."
msgstr "སྒྲོན་བཪྙན་སྤྱིན་ཤོག་གི་འོག་ཤོག་སུ་སྤོ་བ་"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"hd_id3146879\n"
"7\n"
"help.text"
msgid "Go to first slide"
msgstr "ཐོག་མའི་སྒྲོན་བཪྙན་སྤྱིན་ཤོག་ལ་བསྒྱུར་བ་"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"par_id3150479\n"
"48\n"
"help.text"
msgid "Jumps to the first slide in the slide show."
msgstr "སྒྲོན་བཪྙན་སྤྱིན་ཤོག་གི་ཆེས་ཐོག་མའི་ཤོག་གྲངས་ལ་མཆོངས་དགོས།"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"hd_id3148725\n"
"8\n"
