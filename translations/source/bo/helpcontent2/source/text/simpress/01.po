#: 01180001.xhp
msgctxt ""
"01180001.xhp\n"
"par_id3145115\n"
"30\n"
"help.text"
msgid "Select the paper source for your printer."
msgstr "གཏག་པར་ཆས་ཀྱི་ཤོག་བུའི་འབྱུང་ཁུངས་ལ་གདོང་ང་བྱེད་པ་"

