"1\n"
"help.text"
msgid "Interaction"
msgstr "སྒུལ་བསྐྱོད་བཀོད་སྒྲིག་"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"par_id3154762\n"
"2\n"
"help.text"
msgid "<variable id=\"interaktiontext\"><ahelp hid=\".uno:AnimationEffects\">Defines how the selected object behaves when you click on it during a slide show.</ahelp></variable>"
msgstr "<variable id=\"interaktiontext\"><ahelp hid=\".uno:AnimationEffects\">མཚན་ཉིད་བདམས་པའི་ཡུལ་ནི་སྒྲོན་བཪྙན་སྤྱིན་ཤོག་སྟོན་རྒྱུ་གི་བརྒྱུད་རིམ་ནང་གཅིག་གནོན་རྗེས་ཇི་ལྟར་བསྒྱུར་འགྲོ་བ།  </ahelp></variable>"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"hd_id3145116\n"
"3\n"
"help.text"
msgid "Action at mouse click"
msgstr "ཙིག་རྟགས་ལ་རྐྱང་རྡེབ་བྱེད་སྐབས་ཀྱི་སྒུལ་ལས་བྱེད་པ་"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"par_id3153955\n"
"44\n"
"help.text"
msgid "<ahelp hid=\"modules/simpress/ui/interactionpage/listbox\">Specifies the action that will run when you click the selected object during a slide show.</ahelp> You can also assign actions to grouped objects."
msgstr "<ahelp hid=\"modules/simpress/ui/interactionpage/listbox\"> གནས་གཏན་འཁེལ་གྱི་སྒྲོན་བཪྙན་སྤྱིན་ཤོག་སྟོན་རྒྱུ་བསྟོངབསྟོང་པའི་བརྒྱུད་རིམ་ནང་ཡུལ་བདམས་པ།  </ahelp>དེ་གཅིག་གནོན་བྱས་པའི་རྗེས་ལ་འཁོར་རྒྱུགས་བྱེད་འགོ་ཚུགས།"

#: 06070000.xhp
msgctxt ""
"06070000.xhp\n"
"hd_id3150397\n"
"4\n"
"help.text"
msgid "No action"
msgstr "གཡོ་སྒུལ་ལས་མི་བྱེད་པ་"

