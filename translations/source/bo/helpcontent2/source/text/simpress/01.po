msgctxt ""
"01180001.xhp\n"
"par_id3153250\n"
"26\n"
"help.text"
msgid "Page orientation is vertical."
msgstr "ཤོག་ངོས་ཁ་ཕྱོགས་གཞུང་རྒྱུད་དུ་བྱས་པ་"

