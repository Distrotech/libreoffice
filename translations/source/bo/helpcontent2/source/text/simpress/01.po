msgctxt ""
"01180001.xhp\n"
"hd_id3154766\n"
"27\n"
"help.text"
msgid "Landscape"
msgstr "འཕྲེང་ངོས་"

#: 01180001.xhp
msgctxt ""
"01180001.xhp\n"
"par_id3153812\n"
"28\n"
"help.text"
msgid "Page orientation is horizontal."
msgstr "ཤོག་ངོས་ཁ་ཕྱོགས་འཕྲེད་རྒྱུད་དུ་བྱས་"

#: 01180001.xhp
msgctxt ""
"01180001.xhp\n"
"hd_id3153075\n"
