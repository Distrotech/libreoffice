msgstr "<image id=\"img_id3161458\" src=\"cmd/sc_fontwork.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3161458\" xml-lang=\"bo\">རིས་རྟགས་</alt></image>"

