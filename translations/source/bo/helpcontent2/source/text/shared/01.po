msgctxt ""
"05210500.xhp\n"
"hd_id3145669\n"
"5\n"
"help.text"
msgid "Grid"
msgstr "དྲ་རྒྱ།"

#: 05210500.xhp
msgctxt ""
"05210500.xhp\n"
"par_id3150774\n"
"6\n"
"help.text"
msgid "To enable this editor, select the <emph>Blank</emph> bitmap in the bitmap list."
msgstr "རྩོམ་སྒྲིག་ཆས་དེ་སྒུལ་སློང་བྱ་དགོས་ན་གནས་རིས་རེའུའགོད་ནང་<emph>\"སྟོང་ཆད་\"</emph>གནས་རིས་འདེམས་པ།"

#: 05210500.xhp
msgctxt ""
"05210500.xhp\n"
"hd_id3145072\n"
