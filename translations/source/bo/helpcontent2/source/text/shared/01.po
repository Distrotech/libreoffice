msgctxt ""
"02100001.xhp\n"
"par_id3150872\n"
"221\n"
"help.text"
msgid "Represents a printable character."
msgstr "གཏག་པར་བྱ་རུང་གི་ཡིག་རྟགས་ཤིག་ཚབ་བྱེད།"

