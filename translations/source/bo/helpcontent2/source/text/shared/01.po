msgctxt ""
"05030600.xhp\n"
"par_id3154380\n"
"5\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/backgroundpage/selectlb\">Select the type of background that you want to apply.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/backgroundpage/selectlb\">སྤྱོད་དགོས་པའི་རྒྱབ་ལྗོངས་རིགས་འདེམས་པ། </ahelp>"

