msgctxt ""
"06040500.xhp\n"
"hd_id3149283\n"
"8\n"
"help.text"
msgid "Add"
msgstr "གསབ་སྣོན།"

#: 06040500.xhp
msgctxt ""
"06040500.xhp\n"
"par_id3158405\n"
