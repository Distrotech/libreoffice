"par_id3153585\n"
"64\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/menuassignpage/up\">Moves the selected item up one position.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/menuassignpage/up\">འདེམས་གཞི་གོང་ལ་གནས་ས་གཅིག་སྤོས།</ahelp>"

