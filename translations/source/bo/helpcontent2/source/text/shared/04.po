#: 01010000.xhp
msgctxt ""
"01010000.xhp\n"
"par_id3149665\n"
"27\n"
"help.text"
msgid "Moves the focus to the previous section or element in a dialog."
msgstr "འོད་རྟགས་ཉིད་གླེང་སྒྲོམ་ནང་གི་གོང་གི་ཆ་ཤས་སམ་གཞི་རྒྱུ་ལ་སྤོས་པ།"

