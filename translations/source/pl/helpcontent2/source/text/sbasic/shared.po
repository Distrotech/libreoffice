msgstr "<link href=\"text/sbasic/shared/03020205.xhp\" name=\"Instrukcja Write\">Instrukcja Write</link>"

#: 03020205.xhp
msgctxt ""
"03020205.xhp\n"
"par_id3154685\n"
"2\n"
"help.text"
msgid "Writes data to a sequential file."
msgstr "Zapisuje dane do pliku sekwencyjnego."

#: 03020205.xhp
