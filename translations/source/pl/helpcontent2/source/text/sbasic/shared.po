msgctxt ""
"03090103.xhp\n"
"par_id3147560\n"
"4\n"
"help.text"
msgid "IIf (Expression, ExpressionTrue, ExpressionFalse)"
msgstr "IIf (wyrażenie, wyrażenie_prawda, wyrażenie_fałsz)"

