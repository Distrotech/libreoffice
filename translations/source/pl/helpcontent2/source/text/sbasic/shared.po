msgctxt ""
"03132000.xhp\n"
"par_id3143228\n"
"49\n"
"help.text"
msgid "oListener = CreateUnoListener( Prefixname, ListenerInterfaceName )"
msgstr "oListener = CreateUnoListener( nazwa_prefiksu, nazwa_interfejsu_odbiornika)"

#: 03132000.xhp
msgctxt ""
"03132000.xhp\n"
"hd_id3147574\n"
"48\n"
"help.text"
msgid "Example:"
msgstr "Przykład:"

