msgctxt ""
"03090410.xhp\n"
"par_id3153990\n"
"7\n"
"help.text"
msgid "<emph>Expression:</emph> The expression that you want to evaluate."
msgstr "<emph>Wyrażenie:</emph> Wyrażenie, które należy oszacować."

