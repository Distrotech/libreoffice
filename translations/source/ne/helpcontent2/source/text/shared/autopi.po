msgctxt ""
"01150000.xhp\n"
"par_id3150113\n"
"34\n"
"help.text"
msgid "<ahelp hid=\"HID_DLGCONVERT_CHECKBOX1\">Converts the entire document.</ahelp>"
msgstr "<ahelp hid=\"HID_DLGCONVERT_CHECKBOX1\">पुरै कागजात रूपान्तरण गर्दछ ।</ahelp>"

