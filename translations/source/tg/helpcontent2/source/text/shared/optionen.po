"18\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/colorpage/B\">Blue</ahelp>"
msgstr ""

#: 01010500.xhp
msgctxt ""
"01010500.xhp\n"
"par_id3152940\n"
"41\n"
