msgctxt ""
"updatehdl.src\n"
"RID_UPDATE_STR_DOWNLOAD_PAUSE\n"
"string.text"
msgid "Downloading %PRODUCTNAME %NEXTVERSION paused at..."
msgstr "%PRODUCTNAME %NEXTVERSION allalaadimine peatati..."

#: updatehdl.src
msgctxt ""
"updatehdl.src\n"
"RID_UPDATE_STR_DOWNLOAD_ERR\n"
