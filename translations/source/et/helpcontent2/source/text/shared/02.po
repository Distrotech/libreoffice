"par_id273445\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Opens the selected database file for editing.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Avab valitud andmebaasi redigeerimiseks.</ahelp>"

#: 12000000.xhp
