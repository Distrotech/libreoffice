"help.text"
msgid "Form"
msgstr "Vorm"

#: 09070100.xhp
msgctxt ""
"09070100.xhp\n"
"par_id3149167\n"
"25\n"
"help.text"
msgid "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_HYPERLINK_NEWDOCUMENT:LB_FORM\">Specifies whether the hyperlink is inserted as text or as a button.</ahelp>"
msgstr "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_HYPERLINK_NEWDOCUMENT:LB_FORM\">Määrab, kas hüperlink lisatakse dokumenti teksti või nupu kujul.</ahelp>"

