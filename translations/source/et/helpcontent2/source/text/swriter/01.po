msgctxt ""
"05060900.xhp\n"
"par_id3155137\n"
"16\n"
"help.text"
msgid "Specifies print and text options for the selected item."
msgstr "Määrab valitud elemendi teksti ja printimise sätted."

#: 05060900.xhp
msgctxt ""
"05060900.xhp\n"
"hd_id3153345\n"
"18\n"
"help.text"
msgid "Editable in read-only document (frames only)"
