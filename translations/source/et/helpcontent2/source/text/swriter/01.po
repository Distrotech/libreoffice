"par_id3150112\n"
"26\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/tocentriespage/fillchar\">Select the tab leader that you want use.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/tocentriespage/fillchar\">Vali tabeldusmärgi punktiirside, mida soovid kasutada.</ahelp>"

#: 04120221.xhp
