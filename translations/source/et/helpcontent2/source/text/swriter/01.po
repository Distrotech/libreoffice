msgctxt ""
"04120211.xhp\n"
"par_id3143270\n"
"14\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/tocindexpage/level\">Enter the number of heading levels to include in the index.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/tocindexpage/level\">Sisesta pealkirjade tasemete arv, mis registrisse kaasatakse.</ahelp>"

#: 04120211.xhp
msgctxt ""
"04120211.xhp\n"
