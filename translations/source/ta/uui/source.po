#: ids.src
msgctxt ""
"ids.src\n"
"RID_SAVE_PASSWORD\n"
"string.text"
msgid "~Remember password"
msgstr "கடவுச்சொல்லை நினைவில் வை"

#: ids.src
msgctxt ""
"ids.src\n"
"STR_WARNING_BROKENSIGNATURE_TITLE\n"
"string.text"
msgid "Invalid Document Signature"
msgstr "செல்லாத ஆவண கையொப்பம்"

#: ids.src
