msgctxt ""
"05230400.xhp\n"
"hd_id3154497\n"
"3\n"
"help.text"
msgid "Corner Radius"
msgstr "Köşe Yarıçapı"

#: 05230400.xhp
msgctxt ""
"05230400.xhp\n"
"par_id3156027\n"
"4\n"
"help.text"
msgid "You can only round the corners of a rectangular object."
msgstr "Sadece dikdörtgen biçimindeki nesnelerin köşelerini yuvaryalabilirsiniz."

#: 05230400.xhp
msgctxt ""
"05230400.xhp\n"
"hd_id3153935\n"
"5\n"
"help.text"
msgid "Radius"
msgstr "Yarıçap"

