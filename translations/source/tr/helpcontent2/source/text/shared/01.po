msgctxt ""
"02100000.xhp\n"
"par_id00002\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Click to search the next occurrence in downward direction.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Bir sonraki sonucu görüntülemek için tıklayın.</ahelp>"

