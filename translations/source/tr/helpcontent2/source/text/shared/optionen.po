msgstr "<ahelp hid=\".\">Ayda bir güncellemeleri kontrol eder.</ahelp>"

#: online_update.xhp
msgctxt ""
"online_update.xhp\n"
"hd_id1418805\n"
"help.text"
msgid "Check now"
msgstr "Şimdi kontrol et"

