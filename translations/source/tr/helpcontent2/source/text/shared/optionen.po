msgctxt ""
"online_update.xhp\n"
"par_id3031098\n"
"help.text"
msgid "<ahelp hid=\".\">A check will be performed once a day.</ahelp>"
msgstr "<ahelp hid=\".\">Günde bir güncellemeleri kontrol eder.</ahelp>"

