msgctxt ""
"01110000.xhp\n"
"hd_id3145345\n"
"1\n"
"help.text"
msgid "Chart options"
msgstr "Çizelge seçenekleri"

#: 01110000.xhp
msgctxt ""
"01110000.xhp\n"
"par_id3149182\n"
"2\n"
"help.text"
msgid "<variable id=\"farbe\"><ahelp hid=\"\" visibility=\"visible\">Defines the general settings for charts.</ahelp></variable>"
msgstr "<variable id=\"farbe\"><ahelp hid=\"\" visibility=\"visible\">Çizelge için genel ayarları tanımlar.</ahelp></variable>"

