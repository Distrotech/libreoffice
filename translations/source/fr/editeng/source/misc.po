msgid "The dictionary is already full."
msgstr "Le dictionnaire est plein."

#: lingu.src
msgctxt ""
"lingu.src\n"
"RID_SVXSTR_DIC_ERR_READONLY\n"
"string.text"
msgid "The dictionary is read-only."
msgstr "Le dictionnaire est en lecture seule."
