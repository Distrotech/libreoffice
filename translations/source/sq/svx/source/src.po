"RID_ERRHDL\n"
"ERRCODE_IO_INVALIDLENGTH\n"
"string.text"
msgid "Invalid data length."
msgstr "Gjatësia e të dhënave është e pavlefshme."

#: errtxt.src
msgctxt ""
