msgctxt ""
"04060185.xhp\n"
"par_id3159184\n"
"108\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_TTEST\">Returns the probability associated with a Student's t-Test.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_TTEST\">Gir sannsynet som høyrer til ein Student-T-test.</ahelp>"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
