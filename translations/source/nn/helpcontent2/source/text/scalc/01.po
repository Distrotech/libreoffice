msgctxt ""
"04060106.xhp\n"
"par_id3156205\n"
"680\n"
"help.text"
msgid "GCD_ADD(Number(s))"
msgstr "SFF_ADD(Tal)"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3145150\n"
"681\n"
"help.text"
msgid "<emph>Number(s)</emph> is a list of up to 30 numbers."
msgstr "<emph>Tal</emph> er ei liste med opp til 30 tal."

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"hd_id3150239\n"
"682\n"
"help.text"
msgid "Example"
msgstr "Eksempel"

