"bm_id3155370\n"
"help.text"
msgid "<bookmark_value>calculating; accumulated interests</bookmark_value><bookmark_value>accumulated interests</bookmark_value><bookmark_value>CUMIPMT function</bookmark_value>"
msgstr "<bookmark_value>rekna ut; samla rente</bookmark_value><bookmark_value>samla rente</bookmark_value><bookmark_value>SAMLA.RENTE-funksjonen</bookmark_value>"

