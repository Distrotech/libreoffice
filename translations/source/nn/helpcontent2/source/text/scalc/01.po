msgstr "<bookmark_value>SUMPRODUKT-funksjonen</bookmark_value><bookmark_value>skalarprodukt</bookmark_value><bookmark_value>prikkprodukt</bookmark_value><bookmark_value>indre produkt</bookmark_value>"

