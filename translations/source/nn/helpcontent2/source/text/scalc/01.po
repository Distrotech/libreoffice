msgctxt ""
"04060104.xhp\n"
"par_id3156204\n"
"168\n"
"help.text"
msgid "Returns the number of the referenced column."
msgstr "Gir nummeret på den refererte kolonnen."

#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"par_id3150094\n"
"162\n"
"help.text"
msgid "<item type=\"input\">=CELL(\"COL\";D2)</item> returns 4."
