msgctxt ""
"04060107.xhp\n"
"par_idN10D65\n"
"help.text"
msgid "A"
msgstr "A"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_idN10D6B\n"
"help.text"
msgid "B (formula)"
msgstr "B (formel)"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_idN10B75\n"
"help.text"
msgid "B (result)"
msgstr "B (resultat)"

