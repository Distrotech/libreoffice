msgid "YEARS(StartDate; EndDate; Type)"
msgstr "TAL.PÅ.ÅR(Startdato; Sluttdato; Type)"

#: 04060111.xhp
msgctxt ""
"04060111.xhp\n"
"par_id3145387\n"
"25\n"
"help.text"
msgid "<emph>StartDate</emph> is the first date"
msgstr "<emph>Startdato</emph> er den første datoen"

