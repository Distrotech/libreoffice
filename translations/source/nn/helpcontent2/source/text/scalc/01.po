"par_id5627307\n"
"help.text"
msgid "<item type=\"input\">=ISEVEN(3.999)</item> returns FALSE"
msgstr "<item type=\"input\">=ERPARTAL(3,999)</item> returnerar USANN."

#: 04060106.xhp
