msgctxt ""
"04060106.xhp\n"
"par_id3153257\n"
"678\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_GCD\"> The result is the greatest common divisor of a list of numbers.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_GCD\">Gir den største felles nemnaren for ei liste av tal.</ahelp>"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"hd_id3147548\n"
"679\n"
"help.text"
msgid "Syntax"
msgstr "Syntaks"

