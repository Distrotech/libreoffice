msgctxt ""
"04060106.xhp\n"
"par_id3146020\n"
"25\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_ABRUNDEN\">Rounds a number down, toward zero, to a certain precision.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_ABRUNDEN\">Avrunder eit tal nedover mot null med oppgitt presisjon.</ahelp>"

