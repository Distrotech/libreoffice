"04060106.xhp\n"
"par_id3154476\n"
"216\n"
"help.text"
msgid "<item type=\"input\">=FACT(3)</item> returns 6."
msgstr "<item type=\"input\">=FAKULTET(3)</item> returnerar 6."

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3147525\n"
"214\n"
"help.text"
msgid "<item type=\"input\">=FACT(0)</item> returns 1."
msgstr "<item type=\"input\">=IMABS(\"5+12j\")</item> returnerer 13."

