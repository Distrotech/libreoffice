"04060107.xhp\n"
"par_idN10DBC\n"
"help.text"
msgid "{=IF(A1:A3>0;\"yes\";\"no\")}"
msgstr "{=DERSOM(A1:A3>0; \"Ja\"; \"Nei\")}"

