msgctxt ""
"04060106.xhp\n"
"par_id3159192\n"
"683\n"
"help.text"
msgid "<item type=\"input\">=GCD_ADD(5;15;25)</item> returns 5."
msgstr "<item type=\"input\">=IMABS(\"5+12j\")</item> returnerer 13."

#: 04060106.xhp
msgctxt ""
