msgctxt ""
"04060110.xhp\n"
"par_id3156263\n"
"85\n"
"help.text"
msgid "<item type=\"input\">=EXACT(\"microsystems\";\"Microsystems\")</item> returns FALSE."
msgstr "<item type=\"input\">=NØYAKTIG(\"microsystems\"; \"Microsystems\")</item> GIR USANN."

