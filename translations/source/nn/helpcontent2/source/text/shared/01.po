msgctxt ""
"05070100.xhp\n"
"hd_id3147069\n"
"1\n"
"help.text"
msgid "<link href=\"text/shared/01/05070100.xhp\" name=\"Align Left\">Align Left</link>"
msgstr "<link href=\"text/shared/01/05990000.xhp\" name=\"Text\">Tekst</link>"

