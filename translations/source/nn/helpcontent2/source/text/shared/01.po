msgctxt ""
"xformsdatatab.xhp\n"
"hd_id7594225\n"
"help.text"
msgid "Min. (inclusive)"
msgstr "Minst (inklusiv)"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"par_id8147221\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies an inclusive lower bound for values.</ahelp>"
msgstr "<ahelp hid=\".\">Bestemmer ei inklusiv nedre grense for verdiar.</ahelp>"

