msgctxt ""
"xformsdatatab.xhp\n"
"par_id7599108\n"
"help.text"
msgid "<ahelp hid=\".\">Specifies an inclusive upper bound for values.</ahelp>"
msgstr "<ahelp hid=\".\">Angjev ei inklusiv øvre grense for verdiar.</ahelp>"

