msgctxt ""
"06040100.xhp\n"
"par_id714438\n"
"help.text"
msgid "A -B (A, space, minus, B)"
msgstr "A -B (A, mellomrom, minus, B)"

#: 06040100.xhp
msgctxt ""
"06040100.xhp\n"
"par_id3924985\n"
"help.text"
msgid "A -B (unchanged)"
msgstr "A -B (uendra)"

#: 06040100.xhp
msgctxt ""
"06040100.xhp\n"
"par_id1486861\n"
"help.text"
msgid "A --B (A, space, minus, minus, B)"
msgstr "A --B (A, mellomrom, minus, minus, B)"

