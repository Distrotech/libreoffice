msgctxt ""
"xformsdatatab.xhp\n"
"par_id3341776\n"
"help.text"
msgid "<ahelp hid=\".\">Declares the item as a constraint.</ahelp>"
msgstr "<ahelp hid=\".\">Viser at elementet er avgrensa.</ahelp>"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"hd_id5947141\n"
"help.text"
msgid "Calculation"
msgstr "Utrekning"

