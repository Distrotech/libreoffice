msgid "Read-only"
msgstr "Skriveverna"

#: xformsdatatab.xhp
msgctxt ""
"xformsdatatab.xhp\n"
"par_id4569231\n"
"help.text"
msgid "<ahelp hid=\".\">Declares the item as read-only.</ahelp>"
