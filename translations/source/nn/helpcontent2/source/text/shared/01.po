"02070000.xhp\n"
"hd_id3150791\n"
"49\n"
"help.text"
msgid "<switchinline select=\"appl\"><caseinline select=\"CALC\">Formulas </caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\">Formlar </caseinline></switchinline>"

#: 02070000.xhp
