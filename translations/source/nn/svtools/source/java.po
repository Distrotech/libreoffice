"STR_WARNING_INVALIDJAVASETTINGS\n"
"string.text"
msgid "Select JRE"
msgstr "Vel Java-køyremiljø"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_QUESTION_JAVADISABLED\n"
"string.text"
msgid "Enable JRE"
msgstr "Bruk Java"

